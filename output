export sdb=CIDPR
export tdb=CIDUT
export shost=nps-pr
export thost=nps-ut
export sschema=FCBAPP
export tschema=FCBAPP
export SourceWhereClause="CM_date_key IN ('27801','27771','27740')"
export tables='
FACT_ACCOUNT_PROFITABILITY
FACT_CUSTOMER_PROFITABILITY
FACT_RMU_PROFITABILITY
FACT_ACCOUNT_CUSTOMER_RMU_PROFITABILITY
'
for table in $tables ; do
nzsql $tdb -host $thost -c "drop table $tschema.$table"

nohup nz_migrate -sTable $table -tTable $table_20251208_BKP -u admin -shost $shost -thost $thost -sdb $sdb -tdb $tdb -spassword $spassword -tpassword $tpassword -schema $sschema -tschema $tschema -format binary -CreateTargetTable yes  -threads 4 -cksum Yes -genStats FULL 2>&1 | tee /export/home/nz/bwv78/copy-$tbl.log &
done

