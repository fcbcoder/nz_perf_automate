select b.CLIENT_OS_USERNAME, b.SESSION_USERNAME, a.qs_sessionid, priority_name, dbname, schema, 
substring(a.qs_sql, 1, 100), qs_tsubmit, qs_tstart, a.qs_estcost, a.qs_estmem, a.qs_estdisk, a.qs_resrows, a.qs_resbytes
  from _v_qrystat a
       left join _v_session_detail b
       on b.session_id = a.qs_sessionid
       where DBNAME != 'null'
--      and CLIENT_OS_USERNAME = 'pd_dbfraudadm-svc'
--      and DBNAME = 'STGPR'
--      and SESSION_USERNAME like '%FRAUD%'
order by qs_estcost desc
  ;
